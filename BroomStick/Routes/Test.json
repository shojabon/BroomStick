{
  "routes": [
    {
      "route": {
        "path": "/shop/<username>/info"
      },
      "group": {
        "allowedGroups": [ "Admin" ]
      },
      "cache": {
        "interval": 1
      },
      "backends": {
        "endpoints": ["http://127.0.0.1:80"]
      }
    },
    {
      "route": {
        "path": "/test"
      },
      "backends": {
        "endpoints": ["http://127.0.0.1:4567/v1/server/scommand"]
      }
    }
  ],
  "default": {
    "route": {
      "path": null,
      "removeRequestPrefix": true
    },
    "group": {
      "allowedGroups": [ "Admin" ]
    },
    "cache": {
      "interval": 0
    },
    "backends": {
      "endpoints": []
    }
  }
}