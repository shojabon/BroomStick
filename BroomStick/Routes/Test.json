{
  "routes": [
    {
      "route": {
        "path": "/shop/<username>/info"
      },
      "group": {
        "allowedGroups": ["Admin", "User", "Guest"]
      },
      "cache": {
        "interval": 10
      },
      "backends": {
        "endpoints": [ "http://192.168.1.108:80" ]
      }
    }
  ],
  "default": {
    "route": {
      "path": null,
      "removeRequestPrefix": true
    },
    "group": {
      "allowedGroups": [ "Admin" ]
    },
    "cache": {
      "interval": 1,
      "TrackUser": true
    },
    "backends": {
      "endpoints": []
    }
  }
}